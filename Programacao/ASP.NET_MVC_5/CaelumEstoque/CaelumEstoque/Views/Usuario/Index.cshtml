@model IList<CaelumEstoque.Models.Usuario>
<div>
    <table class="table table-hover">
        <thead>
            <tr>
                <th>ID</th>
                <th>Nome</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var produto in Model)
            {
                <tr>
                    <td>@produto.Id</td>
                    @*<td><a href="/Produto/Visualiza/@produto.Id">@produto.Nome</a></td>*@
                    @*<td>@Html.ActionLink(produto.Nome, "Visualiza", new { id = produto.Id })</td>*@
                    <td>@Html.RouteLink(produto.Nome, "VisualizaProduto", new { id = produto.Id })</td>
                </tr>
            }
        </tbody>
    </table>
</div>